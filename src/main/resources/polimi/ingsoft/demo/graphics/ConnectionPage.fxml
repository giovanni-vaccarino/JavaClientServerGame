<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.BackgroundFill?>
<?import javafx.scene.layout.Background?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.layout.VBox?>
<AnchorPane styleClass="page" stylesheets="/polimi/ingsoft/demo/graphics/css/PageStyle.css"
            fx:id="connectionPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="polimi.ingsoft.client.ui.gui.ConnectionPageController"
            maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="800.0" prefWidth="1350.0"
>
    <children>
        <StackPane layoutX="0.0" layoutY="250.0" prefWidth="1350" prefHeight="100" alignment="CENTER">
            <children>
                <Label text="Connection" styleClass="textBold"
                       stylesheets="/polimi/ingsoft/demo/graphics/css/TextStyle.css">
                </Label>
            </children>
        </StackPane>

        <StackPane layoutX="0.0" layoutY="370.0" prefWidth="1350" prefHeight="100" alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" text="rmi"
                        prefWidth="90" prefHeight="30" styleClass="buttonRegular"
                        stylesheets="/polimi/ingsoft/demo/graphics/css/ButtonStyle.css"
                        onAction="#nextPageRMI">
                </Button>
            </children>
        </StackPane>

        <StackPane layoutX="0.0" layoutY="465.0" prefWidth="1350" prefHeight="100" alignment="CENTER">
            <children>
                <Button mnemonicParsing="false" text="Socket"
                        prefWidth="120" prefHeight="30" styleClass="buttonRegular"
                        stylesheets="/polimi/ingsoft/demo/graphics/css/ButtonStyle.css"
                        onAction="#nextPageSocket">
                </Button>
            </children>
        </StackPane>

    </children>
</AnchorPane>
